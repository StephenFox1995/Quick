<html ng-app="signUp">
<head>
    <script src="http://ajax.googleapis.com/ajax/libs/angularjs/1.2.17/angular.min.js"></script>
    <script src="../javascripts/signUp.js"></script>
    <% include /partials/header %>
    <title>
        Index Page
    </title>
</head>

<body>

<div ng-controller="signUpController">
    <h1>Register as business!</h1>
    <p>Name</p>
    <input type="text" ng-model="httpBody.business.name">
    <p>Address</p>
    <input type="text" ng-model="httpBody.business.address">
    <p>Contact Number</p>
    <input type="text" ng-model="httpBody.business.contactNumber">
    <p>Email</p>
    <input type="text" ng-model="httpBody.business.email">
    <p>Password</p>
    <input type="text" ng-model="httpBody.business.password">

    <button type="submit"
            ng-click="signUp()"
            ng-model="signUpModel.endPoint"
            ng-init="signUpModel.endPoint='/business'">Sign Up</button >
    {{message}}
</div>

</body>
</html>

